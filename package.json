{
  "name": "@kherge/result",
  "version": "0.1.0",
  "description": "Bringing Rust's Option<T> and Result<T, E> to TypeScript.",
  "keywords": [
    "rust",
    "option",
    "result",
    "typescript"
  ],
  "homepage": "https://github.com/kherge/js.result",
  "bugs": "https://github.com/kherge/js.result/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kherge/js.result.git"
  },
  "license": "MIT",
  "author": "Kevin Herrera <kevin@herrera.io>",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "husky": "^5.1.3",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "tsdx build",
    "build:docs": "typedoc --options typedoc.json src/index.ts",
    "lint": "tsdx lint",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "main": "dist/index.js",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/result.esm.js",
  "size-limit": [
    {
      "path": "dist/result.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/result.esm.js",
      "limit": "10 KB"
    }
  ],
  "typings": "dist/index.d.ts"
}
